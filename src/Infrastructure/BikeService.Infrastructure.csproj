        <Nullable>enable</Nullable>
        <PackageId>bikeservice-infrastructure</PackageId>
        <Title>BikeService Infrastructure</Title>
        <Version>4.1.0</Version>
        <Authors>Vidar André Bø</Authors>
        <Product>BikeService</Product>
