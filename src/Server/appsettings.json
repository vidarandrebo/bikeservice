{
  "Urls": "http://localhost:8080",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "LogFile": "/var/log/bikeservice/log.txt"
  },
  "CacheMaxAge": 604800,
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": ""
  },
  "DefaultBikeTypes": [
    "Road",
    "XC",
    "TT",
    "Hybrid",
    "Downhill",
    "City",
    "Fatbike",
    "Gravel"
  ],
  "DefaultPartTypes": [
    "Front Wheel",
    "Rear Wheel",
    "Front Derailleur",
    "Rear Derailleur",
    "Front Brake",
    "Rear Brake",
    "Pedals",
    "Chain",
    "Seatpost",
    "Crankset",
    "Handlebar",
    "Saddle"
  ],
  "Jwt": {
    "ValidAudience": "bs.vidarboe.com",
    "ValidIssuer": "bs.vidarboe.com"
  },
  "CancellationToken": {
    "Delay": 2000
  }
}
