{
    "name": "bikeservice-web-ui",
    "version": "3.1.1",
    "private": true,
    "author": {
        "name": "Vidar André Bø"
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "rimraf --glob ../WebAPI/wwwroot/* && vue-tsc && vite build --mode development && cp -r dist/* ../WebAPI/wwwroot/",
        "buildRelease": "vue-tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext ts,vue --report-unused-disable-directives --max-warnings 0",
        "format": "prettier . --write"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.5.2",
        "@fortawesome/free-regular-svg-icons": "^6.5.2",
        "@fortawesome/free-solid-svg-icons": "^6.5.2",
        "@fortawesome/vue-fontawesome": "^3.0.6",
        "vue": "^3.4.23",
        "vue-router": "^4.3.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "@vitejs/plugin-vue": "^5.0.4",
        "autoprefixer": "^10.4.19",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.25.0",
        "postcss": "^8.4.38",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5",
        "tailwindcss": "^3.4.3",
        "typescript": "^5.4.5",
        "vite": "^5.2.9",
        "vue-tsc": "^2.0.13"
    }
}
