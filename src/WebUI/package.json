{
    "name": "bikeservice-web-ui",
    "version": "3.0.4",
    "private": true,
    "author": {
        "name": "Vidar André Bø"
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "rimraf --glob ../WebAPI/wwwroot/* && vue-tsc && vite build --mode development && cp -r dist/* ../WebAPI/wwwroot/",
        "buildRelease": "vue-tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext ts,vue --report-unused-disable-directives --max-warnings 0",
        "format": "prettier . --write"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.5.1",
        "@fortawesome/free-regular-svg-icons": "^6.5.1",
        "@fortawesome/free-solid-svg-icons": "^6.5.1",
        "@fortawesome/vue-fontawesome": "^3.0.6",
        "vue": "^3.4.20",
        "vue-router": "^4.3.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "@vitejs/plugin-vue": "^5.0.4",
        "autoprefixer": "^10.4.17",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.22.0",
        "postcss": "^8.4.35",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.3.3",
        "vite": "^5.1.4",
        "vue-tsc": "^1.8.27"
    }
}
