{
    "name": "bikeservice-client",
    "version": "4.0.0",
    "private": true,
    "author": {
        "name": "Vidar André Bø"
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "rimraf --glob ../Server/wwwroot/* && vue-tsc && vite build --mode development && cp -r dist/* ../Server/wwwroot/",
        "buildRelease": "vue-tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext ts,vue --report-unused-disable-directives --max-warnings 0",
        "format": "prettier . --write"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.6.0",
        "@fortawesome/free-regular-svg-icons": "^6.6.0",
        "@fortawesome/free-solid-svg-icons": "^6.6.0",
        "@fortawesome/vue-fontawesome": "^3.0.8",
        "aspnetcore-ts": "github:vidarandrebo/aspnetcore-ts#v1.0.0",
        "http-methods-ts": "github:vidarandrebo/http-methods-ts#v0.3.0",
        "vue": "^3.5.4",
        "vue-router": "^4.4.4"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.5.0",
        "@typescript-eslint/parser": "^8.5.0",
        "@vitejs/plugin-vue": "^5.1.3",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.28.0",
        "postcss": "^8.4.45",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "tailwindcss": "^3.4.11",
        "typescript": "^5.6.2",
        "vite": "^5.4.4",
        "vue-tsc": "^2.1.6"
    }
}
