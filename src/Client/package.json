{
    "name": "bikeservice-client",
    "version": "4.1.0",
    "private": true,
    "author": {
        "name": "Vidar André Bø"
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "buildDev": "vue-tsc && vite build --mode development",
        "build": "vue-tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
        "lint-fix": "eslint . --report-unused-disable-directives --max-warnings 0 --fix",
        "format": "prettier . --write",
        "generate": "npm run openapi && npm run lint-fix && npm run format",
        "openapi": "openapi-generator-cli generate -i ../../BikeService.Server.json -g typescript-fetch -o src/Gen/ --additional-properties=withInterfaces=true",
        "test": "vitest run"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-svg-core": "^7.0.1",
        "@fortawesome/free-regular-svg-icons": "^7.0.1",
        "@fortawesome/free-solid-svg-icons": "^7.0.1",
        "@fortawesome/vue-fontawesome": "^3.1.2",
        "@openapitools/openapi-generator-cli": "^2.23.4",
        "@typescript-eslint/eslint-plugin": "^8.44.0",
        "@typescript-eslint/parser": "^8.44.0",
        "@vitejs/plugin-vue": "^6.0.1",
        "autoprefixer": "^10.4.21",
        "bulma": "^1.0.4",
        "eslint": "^9.35.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-vue": "^10.4.0",
        "globals": "^16.4.0",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "sass": "^1.93.2",
        "tailwindcss": "^3.4.16",
        "typescript": "~5.9.2",
        "typescript-eslint": "^8.44.0",
        "vite": "^7.1.5",
        "vitest": "^3.2.4",
        "vue": "^3.5.21",
        "vue-router": "^4.5.1",
        "vue-tsc": "^3.0.7"
    }
}
