{
    "name": "bikeservice-client",
    "version": "3.4.0",
    "private": true,
    "author": {
        "name": "Vidar André Bø"
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "rimraf --glob ../Server/wwwroot/* && vue-tsc && vite build --mode development && cp -r dist/* ../Server/wwwroot/",
        "buildRelease": "vue-tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext ts,vue --report-unused-disable-directives --max-warnings 0",
        "format": "prettier . --write"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.6.0",
        "@fortawesome/free-regular-svg-icons": "^6.6.0",
        "@fortawesome/free-solid-svg-icons": "^6.6.0",
        "@fortawesome/vue-fontawesome": "^3.0.8",
        "vue": "^3.4.34",
        "vue-router": "^4.4.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^7.17.0",
        "@typescript-eslint/parser": "^7.17.0",
        "@vitejs/plugin-vue": "^5.1.0",
        "autoprefixer": "^10.4.19",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.27.0",
        "postcss": "^8.4.40",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "tailwindcss": "^3.4.7",
        "typescript": "^5.5.4",
        "vite": "^5.3.5",
        "vue-tsc": "^2.0.29"
    }
}
