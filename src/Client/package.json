{
    "name": "bikeservice-client",
    "version": "4.1.0",
    "private": true,
    "author": {
        "name": "Vidar André Bø"
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "buildDev": "rimraf --glob ../Server/wwwroot/* && vue-tsc && vite build --mode development && cp -r dist/* ../Server/wwwroot/",
        "build": "vue-tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext ts,vue --report-unused-disable-directives --max-warnings 0",
        "format": "prettier . --write"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.7.1",
        "@fortawesome/free-regular-svg-icons": "^6.7.1",
        "@fortawesome/free-solid-svg-icons": "^6.7.1",
        "@fortawesome/vue-fontawesome": "^3.0.8",
        "aspnetcore-ts": "github:vidarandrebo/aspnetcore-ts#v1.0.0",
        "http-methods-ts": "github:vidarandrebo/http-methods-ts#v0.3.0",
        "vue": "^3.5.13",
        "vue-router": "^4.5.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.18.0",
        "@typescript-eslint/parser": "^8.18.0",
        "@vitejs/plugin-vue": "^5.2.1",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.32.0",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "rimraf": "^6.0.1",
        "tailwindcss": "^3.4.16",
        "typescript": "~5.6.3",
        "vite": "^6.0.3",
        "vue-tsc": "^2.1.10"
    }
}
